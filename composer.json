{
    "name": "chamilo/chash",
    "type": "library",
    "description" : "Chamilo Shell",
    "homepage": "http://www.chamilo.org",
    "license": "GPL-3.0-only",
    "support": {
        "forum": "http://www.chamilo.org/forum",
        "irc": "irc://irc.freenode.org/chamilo"
    },
    "autoload": {
        "psr-4": {
            "Chash\\": "src/"
        }
    },
    "require": {
        "php": "^7.2.5",
        "composer/composer": "~1.10.23 || ~2.1.9",
        "symfony/class-loader": "~3.0",
        "symfony/doctrine-bridge": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/process": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/framework-bundle": "~4.0",
        "sonata-project/user-bundle": "~4.0",
        "gedmo/doctrine-extensions": "~2.4",
        "doctrine/dbal": "~2.9",
        "doctrine/orm": "~2.6",
        "doctrine/migrations": "~2.2",
        "zendframework/zend-config": "~3.0",
        "zendframework/zend-stdlib": "~3.0",
        "alchemy/zippy": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mikey179/vfsstream": ">=1.2",
        "symplify/easy-coding-standard": "^7.1",
        "vimeo/psalm": "^3.7",
        "phpstan/phpstan": "^0.12.2"
    },
    "scripts": {
        "analyse": [
            "@php vendor/bin/ecs check src"
        ],
        "fix": [
            "@php vendor/bin/ecs check src --fix"
        ]
    }
}
